<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.omg">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home.MainActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PlaceOrder.PlaceOrderActivity"
            android:label="@string/title_activity_place_order"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Account.LoginActivity" />
        <activity android:name=".Home.SearchActivity"
            android:theme="@style/Transparent"
            android:parentActivityName=".Home.MainActivity"></activity>
    </application>

</manifest>